{
  "name": "FRAwizard_floorplan_app",
  "version": "2.0.6",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "start": "SET NODE_OPTIONS=--openssl-legacy-provider && webpack-dev-server --inline --config demo/webpack.config.js --port 9000 --mode development",
    "build-demo": "webpack --config demo/webpack.config.js --mode production",
    "build-commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build-es": "cross-env BABEL_ENV=es babel src --copy-files --out-dir es",
    "build": "npm run clean && npm run build-demo && npm run build-commonjs && npm run build-es",
    "add-build-to-vcs": "git add -A src/version.js && git add -A es && git add -A lib && git add -A demo/dist",
    "clean": "rimraf lib es demo/dist",
    "website-update": "gh-pages -m 'Update gh-pages' -d ./demo/dist/",
    "update-version-file": "const-version ./package.json ./src/version.js",
    "version": "npm run update-version-file && npm run build && npm run add-build-to-vcs",
    "postpublish": "npm run website-update",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "floorplanner",
    "plan",
    "svg",
    "react",
    "react.js",
    "ui",
    "interface",
    "component"
  ],
  "dependencies": {
    "@mapbox/react-range": "0.0.7",
    "area-polygon": "1.0.1",
    "axios": "^1.6.7",
    "convert-units": "2.3.4",
    "html-webpack-plugin": "3.2.0",
    "immutable": "3.8.2",
    "immutablediff": "0.4.4",
    "immutablepatch": "0.5.0",
    "polylabel": "1.0.2",
    "prop-types": "15.7.2",
    "react-confirm-alert": "^2.8.0",
    "react-icons": "^5.0.1",
    "react-redux": "5.0.7",
    "react-responsive-modal": "^6.4.2",
    "react-select": "^5.8.0",
    "react-svg-loader": "^3.0.3",
    "react-svg-pan-zoom": "2.18.0",
    "react-tabs": "3.0.0",
    "redux": "4.0.1",
    "serve": "^14.2.1",
    "shortid": "2.2.14",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.23.9",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-import-glob": "2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "^6.0.15",
    "const-version": "2.0.0",
    "cross-env": "5.2.0",
    "file-loader": "3.0.1",
    "gh-pages": "2.0.1",
    "immutable-devtools": "0.1.4",
    "react": "16.8.5",
    "react-container-dimensions": "1.4.1",
    "react-dom": "16.8.5",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "three": "0.94.0",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.7.1"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x",
    "three": "0.94.x"
  }
